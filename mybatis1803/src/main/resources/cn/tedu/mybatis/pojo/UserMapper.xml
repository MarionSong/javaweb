<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.tedu.mybatis.pojo.UserMapper">
	<select id="find" resultType="cn.tedu.mybatis.pojo.User">
	select * from user_c
	</select>
	<insert id="insert">
	insert into user_c values ('6','宋铭',20,'吉林');
	</insert>
	<update id="update" parameterType="string">
	update user_c set age=24 where name=#{name}	
	</update>
	<delete id="delete">
	delete from user_c where id='6'
	</delete>
	<select id="get" parameterMap="map" resultType="cn.tedu.mybatis.pojo.User">
	select * from user_c where name=#{name}
	
	</select>
	
	
	
</mapper>